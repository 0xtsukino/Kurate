{
	"name": "kurate",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"build2": "webpack --config webpack.config.cjs",
		"dev2": "webpack-dev-server --config webpack.config.cjs --hot",
		"dev": "npm run protos && vite dev",
		"build": "npm run protos && vite build",
		"preview": "npm run protos && vite preview",
		"test": "npm run test:unit && npm run test:playwright",
		"test:unit": "npm run protos && vitest",
		"test:playwright": "npm run protos && playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write . && eslint . --fix",
		"waku:start": "docker-compose -f ./tests/docker-compose.yaml up -d",
		"waku:stop": "docker kill tests-waku-1",
		"protos": "protons ./src/lib/protos/*.proto"
	},
	"devDependencies": {
		"@fontsource/source-code-pro": "^4.5.14",
		"@fontsource/source-sans-pro": "^4.5.11",
		"@fontsource/source-serif-pro": "^4.5.9",
		"@playwright/test": "1.30.0",
		"@rollup/plugin-inject": "^5.0.3",
		"@semaphore-protocol/group": "3.0.0",
		"@semaphore-protocol/identity": "3.0.0",
		"@semaphore-protocol/proof": "3.0.0",
		"@sveltejs/adapter-static": "^1.0.6",
		"@sveltejs/kit": "^1.3.10",
		"@types/elliptic": "^6.4.14",
		"@types/page": "^1.11.5",
		"@typescript-eslint/eslint-plugin": "^5.50.0",
		"@typescript-eslint/parser": "^5.50.0",
		"@vue/preload-webpack-plugin": "^2.0.0",
		"@zk-kit/identity": "^1.4.1",
		"@zk-kit/protocols": "^1.11.1",
		"big-integer": "^1.6.51",
		"bigint-conversion": "^2.3.0",
		"browser-or-node": "^2.1.1",
		"browserify": "^17.0.0",
		"css-loader": "^6.7.3",
		"dotenv-webpack": "^8.0.1",
		"elliptic": "^6.5.4",
		"eslint": "^8.33.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"ethers": "^5.7.2",
		"eventemitter2": "^6.4.9",
		"html-webpack-plugin": "^5.5.0",
		"js-waku": "^0.30.0",
		"mini-css-extract-plugin": "^2.7.2",
		"multiaddr": "^10.0.1",
		"node-stdlib-browser": "^1.2.0",
		"page": "^1.11.6",
		"prettier": "^2.8.3",
		"prettier-plugin-svelte": "^2.9.0",
		"protons": "^7.0.0",
		"protons-runtime": "^5.0.0",
		"sass": "^1.58.0",
		"style-loader": "^3.3.1",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.3",
		"svelte-loader": "^3.1.6",
		"svelte-preprocess": "^5.0.1",
		"ts-loader": "^9.4.2",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.1",
		"vitest": "^0.28.5",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.0",
		"webpack-dev-server": "^4.11.1",
		"zkitter-js": "^1.1.5"
	},
	"type": "module"
